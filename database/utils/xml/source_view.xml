                </SELECT_LIST_ITEM>
              </SELECT_LIST>
            </SELECT>
            <FROM>
              <FROM_ITEM>
                <TABLE>EMP</TABLE>
              </FROM_ITEM>
            </FROM>
            <WHERE>
              <GT>
                <COLUMN_REF>
                  <TABLE>EMP</TABLE>
                  <COLUMN>HIREDATE</COLUMN>
                </COLUMN_REF>
                <DATE>1980-01-01</DATE>
              </GT>
            </WHERE>
          </QUERY>
          <TABLE_ALIAS>E</TABLE_ALIAS>
        </JOIN_TABLE_2>
        <ON>
          <EQ>
            <COLUMN_REF>
              <TABLE_ALIAS>E</TABLE_ALIAS>
              <COLUMN>DEPTNO</COLUMN>
            </COLUMN_REF>
            <COLUMN_REF>
              <TABLE>DEPT</TABLE>
              <TABLE_ALIAS>D</TABLE_ALIAS>
              <COLUMN>DEPTNO</COLUMN>
            </COLUMN_REF>
          </EQ>
        </ON>
      </JOIN>
    </FROM_ITEM>
  </FROM>
  <GROUP_BY>
    <EXPRESSION_LIST>
